//动态规划
int numDecodings(char * s){
    int n=strlen(s);
    //f数组里是前 i个 字符的解码方式
    //1个字母所能表示的数字大小不会超过26，也就是两位数
    int f[n+1];
    f[0]=1;
    if(n==0)
        return 0;
    for(int i=1;i<=n;i++)
    {
        //先初始化为0 
        f[i]=0;
        //第i个数字的前一个数字，如果是在1~9之间满足条件
        int t=s[i-1]-'0';
        if(t>=1&&t<=9)
            f[i]+=f[i-1];
        if(i>1)
        {
            //第i个数字的前两个数字，如果在10~26之间满足条件
            t=(s[i-2]-'0')*10+s[i-1]-'0';
            if(t>=10&&t<=26)
                f[i]+=f[i-2];
        }
    }
    return f[n];
}
